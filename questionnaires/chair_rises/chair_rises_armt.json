    {
        "field_name": "chair_rises_instructions_2",
        "form_name": "chair_rises",
        "section_header": "",
        "field_type": "info",
        "field_label": "Chair Rises Instructions\r\n",
        "select_choices_or_calculations": [
            {
                "code": "Part1",
                "label": "Press button A (activity button A is located at the top right-hand side of the watch face). Note! If this is the first time you have started an activity, select the checkbox next to 'Walk' and 'Cardio' to add these activities to your favorites."
            },
            {
                "code": "Part2",
                "label": "Using the watch face touchscreen (C), select 'Cardio'"
            },
            {
                "code": "Part3",
                "label": "'Ready' will appear on the watch face'."
            },
            {
                "code": "Part4",
                "label": "Press button A to start the activity timer'"
            },
            {
                "code": "Part5",
                "label": "Go! Stand up and then sit down as quickly as you can 10 times'"
            },
            {
                "code": "Part6",
                "label": "When you have completed 10 chair rises, Stop!"
            },
            {
                "code": "Part7",
                "label": "Check the clock timer on the watch face and sit still for a 1-minute recovery period. After 1 minute of recovery, press button A to stop the activity timer'."
            },
            {
                "code": "Part8",
                "label": "Save the activity by pressing the green tick on the watch face touchscreen (C)."
            },
            {
                "code": "Part9",
                "label": "Congratulations! You have completed your Chair Rises Test."
            },
            {
                "code": "Part10",
                "label": "Please log your most recent test in the next section."
            }
        ],
